{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-standard"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "space-before-function-paren": [
      true,
      "never"
    ],
    "prefer-const": true,
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "max-line-length": [
      true,
      150
    ],
    "member-ordering": [
      false
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}