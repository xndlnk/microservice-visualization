<idea-plugin>
    <id>de.maibornwolff.tadis-intellij</id>
    <name>TADIS</name>
    <vendor email="andreas.blunk@maibornwolff.de" url="http://www.maibornwolff.de">MaibornWolff</vendor>

    <description><![CDATA[
    This is an IntelliJ plugin for TADIS system analysis and visualization.
    ]]></description>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
        <toolWindow id="Embedded Web Browser" anchor="right" factoryClass="de.maibornwolff.tadis.BrowserWindowFactory"/>
    </extensions>
</idea-plugin>